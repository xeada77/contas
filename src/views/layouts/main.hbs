    <title>Contas</title>
</head>

<body>
    <div class="wrapper">
        {{>navegacion}}
        <main class="w-75 mx-auto">
            {{>errors}}
            <div id="content">
                <nav class="navbar navbar-expand-lg navbar-light bg-light">
                    <div class="">
                        <button type="button" id="sidebarCollapse" class="btn btn-info">
                            <i class="fas fa-align-left"></i>
                            <span id="menuBtnText">Cerrar Menu</span>
